<?xml version="1.0" encoding="utf-8"?>
<android.support.wearable.view.ObservableScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/parentPanel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    android:overScrollMode="never"
    android:scrollbars="none"
    >

    <LinearLayout
        android:id="@+id/animatedWrapperContainer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:clipChildren="false"
        >

        <TextView
            android:id="@+id/alertTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/diag_content_top_padding"
            android:paddingLeft="@dimen/diag_content_side_padding"
            android:paddingRight="@dimen/diag_content_side_padding"
            style="@style/Widget.WearDiag.TextView.Title"
            android:textColor="@color/dark_red"
            />

        <TextView
            android:id="@+id/messageComID"
            android:layout_width="match_parent"
            android:text="ajhdsjdjakdjkasdsskaj"
            android:layout_height="wrap_content"
            android:paddingBottom="@dimen/diag_content_bottom_padding"
            android:paddingLeft="@dimen/diag_content_side_padding"
            android:paddingRight="@dimen/diag_content_side_padding"
            style="@style/Widget.WearDiag.TextView.Message"
            android:textColor="@color/dark_grey"
            />

        <LinearLayout
            android:id="@+id/buttonPanel"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/dialog_shade_background"
            android:orientation="vertical"
            android:paddingLeft="@dimen/diag_content_side_padding"
            android:paddingRight="@dimen/diag_content_side_padding"
            android:paddingBottom="@dimen/diag_button_bottom_padding"
            >

            <Button
                android:id="@+id/button1"
                android:background="@color/dark_red"
                android:textColor="@color/white"
                android:text="OK"
                android:gravity="center"
                android:textStyle="bold"
                android:layout_height="40dp"
                style="@style/Widget.WearDiag.Button"/>

            <Button
                android:id="@+id/button3"
                android:visibility="invisible"
                style="@style/Widget.WearDiag.Button"/>

            <Button
                android:id="@+id/button2"
                android:visibility="invisible"
                style="@style/Widget.WearDiag.Button"/>
        </LinearLayout>
    </LinearLayout>
</android.support.wearable.view.ObservableScrollView>
